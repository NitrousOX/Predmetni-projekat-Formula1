<Window x:Class="Predmetni_projekat_Formula1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Predmetni_projekat_Formula1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="Proizvodjaci">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <TreeView Grid.Column="0" ItemsSource="{Binding proizvodjaciObsLeft}">
                        
                    </TreeView>
                    <Image Grid.Column="1" Source="/Slike/Evropa.JPG" Margin="4"/>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
